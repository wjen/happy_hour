 strIngredient2,\n            strIngredient3,\n            strIngredient4,\n            strIngredient5,\n            strIngredient6,\n          ];\n          const measurements = [\n            strMeasure1,\n            strMeasure2,\n            strMeasure3,\n            strMeasure4,\n            strMeasure5,\n            strMeasure6,\n          ];\n          const newDrink = {\n            name,\n            category,\n            image,\n            glass,\n            instructions,\n            ingredients,\n            measurements,\n          };\n          setIsLoading(false);\n          setDrink(newDrink);\n        } else {\n          setDrink(null);\n        }\n      } catch (error) {\n        console.log(error);\n        setIsLoading(false);\n      }\n    };\n    fetchDrink();\n  }, [id]);\n\n  useEffect(() => {\n    const fetchVideos = async () => {\n      try {\n        console.log(drink.name);\n        const response = await axios.get(\n          `http://localhost:5000/api/drink/videos?name=${drink.name}`\n        );\n        // console.log(response);\n        const videoIds = response.data.items.map((item) => item.id.videoId);\n        // console.log(videoIds);\n        setYouTubeVideoIds(videoIds);\n        // console.log(response);\n      } catch (error) {\n        console.log(error);\n      }\n    };\n    fetchVideos();\n  }, [drink]);\n\n  if (isLoading) {\n    return <Loading />;\n  }\n  if (!drink) {\n    return <h3 className=\"text-center mt-4\">No drink to display</h3>;\n  } else {\n    const {\n      name,\n      image,\n      category,\n      glass,\n      instructions,\n      ingredients,\n      measurements,\n    } = drink;\n\n    return (\n      <section className=\"details\">\n        <div className=\"container\">\n          <div className=\"row\">\n            <div className=\"details__img-container col-sm-6 col-md-6\">\n              <img src={image} alt={name} />\n            </div>\n            <div className=\"details__text col-sm-6 col-md-6\">\n              <p>\n                <span className=\"badge badge-info p-2 mr-3 details__badge\">\n                  Name:\n                </span>\n                {name}\n              </p>\n              <p>\n                <span className=\"badge badge-info p-2 mr-3 details__badge\">\n                  Category:\n                </span>\n                {category}\n              </p>\n              <p>\n                <span className=\"badge badge-info p-2 mr-3 details__badge\">\n                  Glass:\n                </span>\n                {glass}\n              </p>\n              <p>\n                <span className=\"badge badge-info p-2 mr-3 details__badge\">\n                  Instructions:\n                </span>\n                {instructions}\n              </p>\n              <div>\n                <span className=\"badge badge-info p-2 mr-3 details__badge\">\n                  Ingredients List:\n                </span>\n                {ingredients.map((item, idx) => {\n                  return item ? (\n                    <span key={idx} className=\"details__ingredients\">\n                      [{item} - {measurements[idx]}] &nbsp;\n                    </span>\n                  ) : null;\n                })}\n              </div>\n              <div>\n                <h2 className=\"mt-3 details__subtitle\">\n                  \"How To Make {name} Cocktail\" Instructional Videos\n                </h2>\n                <hr />\n                <div className=\"details__videos-container\">\n                  <div className=\"row\">\n                    {youTubeVideoIds &&\n                      youTubeVideoIds.map((videoId) => {\n                        return (\n                          <iframe\n                            className=\"details__video col-lg-6\"\n                            key={videoId}\n                            src={`http://www.youtube.com/embed/${videoId}`}\n                            allowFullScreen\n                          ></iframe>\n                        );\n                      })}\n                  </div>\n                </div>\n              </div>\n\n              <Link to=\"/\" className=\"btn btn-primary mt-3\">\n                back home\n              </Link>\n            </div>\n          </div>\n        </div>\n      </section>\n    );\n  }\n}\n\nexport default SingleDrink;\n","/Users/wenjen/Desktop/node-apps/happy-hour/src/components/Loading.js",[],"/Users/wenjen/Desktop/node-apps/happy-hour/src/pages/Register.js",[],"/Users/wenjen/Desktop/node-apps/happy-hour/src/pages/Login.js",[],"/Users/wenjen/Desktop/node-apps/happy-hour/src/services/auth-header.js",[],["92","93"],"/Users/wenjen/Desktop/node-apps/happy-hour/src/components/MyTextInput.js",[],"/Users/wenjen/Desktop/node-apps/happy-hour/src/pages/Profile.js",[],"/Users/wenjen/Desktop/node-apps/happy-hour/src/services/user.service.js",[],"/Users/wenjen/Desktop/node-apps/happy-hour/src/pages/Users.js",[],"/Users/wenjen/Desktop/node-apps/happy-hour/src/util/AuthRoute.js",[],"/Users/wenjen/Desktop/node-apps/happy-hour/src/context/context.js",[],"/Users/wenjen/Desktop/node-apps/happy-hour/src/context/auth.context.js",[],{"ruleId":"94","replacedBy":"95"},{"ruleId":"96","replacedBy":"97"},{"ruleId":"98","severity":1,"message":"99","line":167,"column":27,"nodeType":"100","endLine":172,"endColumn":28},{"ruleId":"94","replacedBy":"101"},{"ruleId":"96","replacedBy":"102"},"no-native-reassign",["103"],"no-negated-in-lhs",["104"],"jsx-a11y/iframe-has-title","<iframe> elements must have a unique title property.","JSXOpeningElement",["103"],["104"],"no-global-assign","no-unsafe-negation"]
n          const newDrink = {\n            name,\n            category,\n            image,\n            glass,\n            instructions,\n            ingredients,\n            measurements,\n          };\n          setIsLoading(false);\n          setDrink(newDrink);\n        } else {\n          setDrink(null);\n        }\n      } catch (error) {\n        console.log(error);\n        setIsLoading(false);\n      }\n    };\n    fetchDrink();\n  }, [id]);\n\n  useEffect(() => {\n    const fetchVideos = async () => {\n      try {\n        console.log(drink.name);\n        const response = await axios.get(\n          `https://www.googleapis.com/youtube/v3/search?key=YT_API_KEY=&type=video&part=snippet&maxResults=3&q=how+to+make+${drink.name}+cocktail`\n        );\n        const videoIds = response.data.items.map((item) => item.id.videoId);\n        setYouTubeVideoIds(videoIds);\n      } catch (error) {\n        console.log(error);\n      }\n    };\n    fetchVideos();\n  }, [drink]);\n\n  if (isLoading) {\n    return <Loading />;\n  }\n  if (!drink) {\n    return <h3 className=\"text-center mt-4\">No drink to display</h3>;\n  } else {\n    const {\n      name,\n      image,\n      category,\n      glass,\n      instructions,\n      ingredients,\n      measurements,\n    } = drink;\n\n    return (\n      <section className=\"details\">\n        <div className=\"container\">\n          <div className=\"row\">\n            <div className=\"details__img-container col-sm-6 col-md-6\">\n              <img src={image} alt={name} />\n            </div>\n            <div className=\"details__text col-sm-6 col-md-6\">\n              <p>\n                <span className=\"badge badge-info p-2 mr-3 details__badge\">\n                  Name:\n                </span>\n                {name}\n              </p>\n              <p>\n                <span className=\"badge badge-info p-2 mr-3 details__badge\">\n                  Category:\n                </span>\n                {category}\n              </p>\n              <p>\n                <span className=\"badge badge-info p-2 mr-3 details__badge\">\n                  Glass:\n                </span>\n                {glass}\n              </p>\n              <p>\n                <span className=\"badge badge-info p-2 mr-3 details__badge\">\n                  Instructions:\n                </span>\n                {instructions}\n              </p>\n              <div>\n                <span className=\"badge badge-info p-2 mr-3 details__badge\">\n                  Ingredients List:\n                </span>\n                {ingredients.map((item, idx) => {\n                  return item ? (\n                    <>\n                      <span key={idx} className=\"details__ingredients\">\n                        [{item} - {measurements[idx]}] &nbsp;\n                      </span>\n                    </>\n                  ) : null;\n                })}\n              </div>\n              <div>\n                <h2 className=\"mt-3 details__subtitle\">\n                  \"How To Make {name}\" Instructional Videos\n                </h2>\n                <hr />\n                {/* <div className=\"details__videos-container\">\n                  {youTubeVideoIds &&\n                    youTubeVideoIds.map((videoId) => {\n                      return (\n                        <iframe\n                          className=\"details__video\"\n                          key={videoId}\n                          src={`http://www.youtube.com/embed/${videoId}`}\n                          allowFullScreen\n                        ></iframe>\n                      );\n                    })}\n                </div> */}\n              </div>\n\n              <Link to=\"/\" className=\"btn btn-primary mt-3\">\n                back home\n              </Link>\n            </div>\n          </div>\n        </div>\n      </section>\n    );\n  }\n}\n\nexport default SingleDrink;\n","/Users/wenjen/Desktop/node-